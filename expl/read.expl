int main(){
decl 
	int t, fd, eof;
	string file;
	string word;
enddecl
begin
	t = exposcall("Read", -1, file);
	fd = exposcall("Open", file);
	eof = exposcall("Read",fd, word);
	while(eof != -2)do
		t = exposcall("Write", -2, word);
		eof = exposcall("Read",fd, word);
	endwhile;
	t = exposcall("Close",fd);
return 0;
end

}
